{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "plugins": [],
  "rules": {
    "prettier/prettier": [
      2,
      {
        "bracketSpacing": true,
        "printWidth": 840,
        "singleQuote": true,
        "trailingComma": "none",
        "tabWidth": 4,
        "useTabs": false,
        "endOfLine": "auto",
        "noWrap": "(selector \\.| .[a-z]+)"
      }
    ]
  }
}
